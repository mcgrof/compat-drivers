+static int wil_cfg80211_disconnect(struct wiphy *wiphy,
+		struct net_device *ndev,
+		u16 reason_code)
+{
+	int rc;
+	struct wil6210_priv *wil = wiphy_to_wil(wiphy);
+	struct {
